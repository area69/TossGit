@model IEnumerable<TOSS_UPGRADE.Models.FM_Disbursement.CheckInventoryList>
@{
    DB_TOSSEntities SISDB = new DB_TOSSEntities();
}
@using TOSS_UPGRADE.Models;
    <div class="table-responsive">
        <table class="table table-striped table-bordered zero-configuration w-100">
            <thead class="text-center">
                <tr>
                    <th>Bank Name</th>
                    <th>Bank Account No.</th>
                    <th width="5px">Quantity</th>
                    <th>Starting Check No.</th>
                    <th>Ending Check No.</th>
                    <th width="5px;">Action</th>
                </tr>
            </thead>
            <tbody class="text-center">
                @{
                    foreach (var Items in Model)
                    {
                        var DeleteConfirmationMess = "Are you sure do you want to delete this Check Inventory?";
                        var DeleteSuccesMess = "Check Inventory Deleted";

                        var ViewEdit = "<button class=\"btn btn-icon btn-block white bg-addbutton border-0\" type=\"button\"onclick=\"AngularGlobalEdit('FMDisbursementController','Get_UpdateDVType','" + @Items.CheckInvntID + "')\"><i class='ft-edit'></i> Edit</button>";
                        var ViewDelete = "<button class=\"btn btn-icon white btn-block bg-headerbc  border-0\" type=\"button\" onclick=\"swalConfirmation('" + @DeleteConfirmationMess + "', '" + @Items.Bank + "','" + @DeleteSuccesMess + "','AngularGlobalDelete(" + @Items.CheckInvntID + ",&quot;Get_DeleteDVType&quot;,&quot;FMDisbursementController&quot;)')\"> <i class='ft-delete'></i> Delete</button>";
                        var ActionContentProgram = ViewEdit + ViewDelete;
                <tr>
                    <td>@Items.Bank</td>
                    <td>@Items.AccountName</td>
                    <td>@Items.Quantity</td>
                    <td>@Items.StartingChckNo</td>
                    <td>@Items.EndingChckNo</td>
                    <td>@Items.Date</td>
                    <td>
                        <button type="button" onfocusout="PopoveLostFocus()" class="btn btn-m btn-warning popover-trigger" data-toggle="popover" data-content="@ActionContentProgram"
                                data-original-title="" data-html="true" data-placement="right">
                            <i class="la la-bars"></i>
                        </button>
                    </td>
                </tr>
                    }
                }
            </tbody>
        </table>
    </div>
<script>
    $(function () {
        LoadDataTable();
        LoadPopOver();
    })
</script>



@* Check Inventory *@
<script>
    $(document).ready(function () {
        var popupElement =
            '<div class="row m-0"><button class="btn btn-icon bg-addbutton white form-control target-desc t-has-destination2" destination="table-project" style="border-color: white"><i class="la la-clipboard"></i>Update</button></div>'
            + '<div class="row m-0 mt-1"><button class="btn btn-icon bg-headerbc white form-control target-desc t-has-destination2" destination="table-project" style="border-color: white"><i class="la la-clipboard"></i>Delete</button></div>';
        $('[data-toggle="popoverCheckInventory"]').popover({
            animation: true,
            content: popupElement,
            html: true
        });
    });
    function ClosePopOver1() {
        $('[data-toggle="popoverCheckInventory"]').popover('hide');
    }
</script>

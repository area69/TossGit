                                    <div class="card-body box-shadow-2">
                                        <div id="_ORDetailsID">

                                        </div>
                                        <div id="_ORDetailsTableID">

                                        </div>
                                    </div>
                                </div>
                            </div>
                            @* Cash Ticket *@

@model TOSS_UPGRADE.Models.FM_AccountableForm.FM_AccountableForm
@using TOSS_UPGRADE.Models;
@{
    DB_TOSSEntities CEE = new DB_TOSSEntities();
}
@{
    ViewBag.Title = "Accountable Form";
}
<script>
    $("#faccountableform").addClass("active");
</script>

<div class="content-header row">
    <div class="content-header-left col-md-6 mb-2">
        <h3 class="content-header-title black"><b>File Maintenance - Accountable Form</b></h3>
    </div>
</div>
<div class="card-content" id="FMAccountableFormController" ng-controller="FMAccountableFormController" ng-init="InitAllFunction();">
    <div class="card">
        <ul class="nav nav-tabs bg-headerbc nav-underline">
            <li class="nav-item">
                <a class="nav-link active" id="base-tab1" data-toggle="pill" aria-controls="tab1" href="#tab1" aria-expanded="true"><b>Accountable Form Details</b></a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="base-tab2" data-toggle="tab" aria-controls="tab2" href="#tab2" aria-expanded="false"><b>Inventory of Accountable Form</b></a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="base-tab3" data-toggle="tab" aria-controls="tab3" href="#tab3" aria-expanded="false"><b>Assignment of Accountable Form</b></a>
            </li>
        </ul>
        <div class="tab-content px-1 pt-1">
            @* Accountable Form Details *@
            <div role="tabpanel" class="tab-pane active black" id="tab1" aria-expanded="true" aria-labelledby="base-tab1">

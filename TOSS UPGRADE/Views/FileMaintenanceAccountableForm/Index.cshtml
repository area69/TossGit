                                    <div class="card-body box-shadow-2">
                                        <div id="_CTCDetailsID">

                                        </div>
                                        <div id="_CTCDetailsTableID">

                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

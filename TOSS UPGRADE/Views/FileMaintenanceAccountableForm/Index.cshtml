                    <div class="card box-shadow-2" id="_AccountableFormID">

                    </div>
                    <div class="row">
                        <div class="col-md-12" id="_AccountableFormTableID">

                        </div>
                    </div>
                </div>
            </div>
            @* Inventory of Accountable Form *@
